@model IEnumerable<Proiect_1_Sfranciog.Models.Dosare>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Informatii)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Concluzii)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Onorariu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Id_Client)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Informatii)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Concluzii)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Onorariu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Id_Client)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
